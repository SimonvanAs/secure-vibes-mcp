\f2\b0\fs26 \cf0 class ScanStateManager:\
    """Manages scan state and artifact caching"""\
    \
    def __init__(self, base_path: str):\
        self.base_path = Path(base_path)\
        self.artifact_dir = self.base_path / ".securevibes"\
        \
    def get_status(self) -> dict:\
        """Returns current state of all artifacts"""\
        artifacts = [\
            "SECURITY.md",\
            "THREAT_MODEL.json",\
            "VULNERABILITIES.json",\
            "DAST_VALIDATION.json",\
            "scan_results.json"\
        ]\
        \
        status = \{\}\
        for artifact in artifacts:\
            path = self.artifact_dir / artifact\
            if path.exists():\
                status[artifact] = \{\
                    "exists": True,\
                    "last_modified": path.stat().st_mtime,\
                    "size_bytes": path.stat().st_size\
                \}\
            else:\
                status[artifact] = \{"exists": False\}\
        \
        return status\
    \
    def load_artifact(self, name: str) -> Optional[dict]:\
        """Loads and parses artifact content"""\
        path = self.artifact_dir / name\
        if not path.exists():\
            return None\
        \
        if name.endswith(".json"):\
            return json.loads(path.read_text())\
        else:\
            return \{"content": path.read_text()\}\
