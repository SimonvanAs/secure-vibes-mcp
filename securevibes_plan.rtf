        "threat-modeling": ["SECURITY.md"],\
        "code-review": ["THREAT_MODEL.json"],\
        "dast": ["VULNERABILITIES.json"],\
        "report-generator": ["VULNERABILITIES.json"]\
    \}\
    \
    def validate(self, agent: str, path: str) -> tuple[bool, list[str]]:\
        """Returns (is_valid, missing_artifacts)"""\
        required = self.DEPENDENCIES.get(agent, [])\
        missing = []\
        \
        for artifact in required:\
            artifact_path = Path(path) / ".securevibes" / artifact\
            if not artifact_path.exists():\
                missing.append(artifact)\
        \
        return (len(missing) == 0, missing)\
    \
    def get_next_available_agents(self, path: str) -> list[str]:\
        """Returns list of agents that can run given current state"""\
        available = []\
        for agent in self.DEPENDENCIES.keys():\
            is_valid, _ = self.validate(agent, path)\
            if is_valid:\
                available.append(agent)\
        return available\
