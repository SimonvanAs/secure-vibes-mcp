\f2\fs26 \cf0 import asyncio\
from typing import Optional\
from mcp.server import Server\
from mcp.types import Tool, TextContent\
from securevibes import Scanner\
\
class SecureVibesMCPServer:\
    def __init__(self):\
        self.server = Server("securevibes")\
        self._setup_tools()\
        \
    def _setup_tools(self):\
        @self.server.list_tools()\
        async def list_tools() -> list[Tool]:\
            return [\
                Tool(\
                    name="run_assessment",\
                    description="Analyze codebase architecture",\
                    inputSchema=\{...\}\
                ),\
                Tool(\
                    name="run_threat_modeling",\
                    description="Perform STRIDE analysis",\
                    inputSchema=\{...\}\
                ),\
                # ... other tools\
            ]\
        \
        @self.server.call_tool()\
        async def call_tool(name: str, arguments: dict):\
            if name == "run_assessment":\
                return await self._run_assessment(arguments)\
            elif name == "run_threat_modeling":\
                return await self._run_threat_modeling(arguments)\
            # ... handle other tools\
            \
    async def _run_assessment(self, args: dict) -> list[TextContent]:\
        scanner = Scanner(model=args.get("model", "sonnet"))\
        \
        # Check for existing artifact\
        artifact_path = Path(args["path"]) / ".securevibes" / "SECURITY.md"\
        if artifact_path.exists() and not args.get("force"):\
            return [TextContent(\
                type="text",\
                text=f"SECURITY.md already exists. Use force=true to overwrite."\
            )]\
        \
        # Run assessment sub-agent\
        result = await scanner.scan(\
            args["path"],\
            subagent="assessment"\
        )\
        \
        return [TextContent(\
            type="text",\
            text=json.dumps(result.summary, indent=2)\
        )]\
\pard\pardeftab720\sa280\partightenfactor0

